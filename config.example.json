{
  "mcpServers": {
    "concurrent-browser": {
      "command": "npx",
      "args": ["concurrent-browser-mcp", "--max-instances", "20"]
    },
    "concurrent-browser-with-proxy": {
      "command": "npx", 
      "args": ["concurrent-browser-mcp", "--max-instances", "20", "--proxy", "http://127.0.0.1:7890"]
    },
    "concurrent-browser-no-proxy": {
      "command": "npx",
      "args": ["concurrent-browser-mcp", "--max-instances", "20", "--no-proxy-auto-detect"]
    }
  }
} 